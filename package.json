    "scripts": {
        "start": "webpack-dev-server --config webpack.dev.js --open",
        "thick-dev": "webpack-dev-server --config webpack.thick-dev.js --open",
        "build": "rm -rf dist/* && webpack -p --config webpack.build.js",
        "analyse": "rm -rf dist/* && webpack -p --config webpack.analyse.js",
        "dev-deploy": "rm -rf dist/* && webpack -p --config webpack.dev-deploy.js",
