    "autoprefixer": "^10.0.1",
    "aws-sdk": "^2.765.0",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^6.1.1",
    "cross-env": "^7.0.2",
    "css-loader": "^4.3.0",
    "dsv-loader": "^2.0.0",
