  },
  "bugs": {
    "url": "https://github.com/nzherald/template/issues"
  },
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.js --open",
    "prebuild": "rm -rf dist/* server/* && webpack -p --config webpack.ssr.js && ./upload-assets.sh && node compile.js",
    "livetest": "npm run prebuild && webpack -p --config webpack.livetest.js",
    "analyse": "npm run prebuild && webpack -p --config webpack.analyse.js",
    "release": "npm run prebuild && webpack -p --config webpack.release.js && ./upload.sh"
  },
  "browserslist": [
    "last 5 version",
    ">1%",
    "not dead"
  ],
